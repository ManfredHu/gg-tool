{
  "name": "hu-tool",
  "version": "1.2.1",
  "description": "good game tools, means final tool",
  "directories": {},
  "scripts": {
    "dev": "jest --watchAll --no-cache",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "test": "npm run unit",
    "lint": "eslint --fix src test/unit/specs",
    "build": "webpack --config webpack.prod.js",
    "watch": "jest --watch",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "commit": "npx git-cz",
    "pub": "npm run build && npm version patch -f",
    "version": "git add . && git push",
    "postversion": "npm publish"
  },
  "files": [
    "src",
    "lib/*.js"
  ],
  "unpkg": "lib/hu-tool.js",
  "keywords": [
    "util",
    "tool",
    "url",
    "useragent",
    "check",
    "phone"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ManfredHu/hu-tool.git"
  },
  "author": "manfredhu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ManfredHu/hu-tool/issues"
  },
  "homepage": "https://github.com/ManfredHu/hu-tool#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "is-type-of": "^1.2.1",
    "url-parse": "^1.4.3"
  },
  "main": "lib/hu-tool.js",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "typings": "types/index.d.ts"
}
